<div>Welcome to MyAureliaApp</div>
<div aurelia-app></div>

@section Scripts {
    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/jqueri-ui/jquery-ui.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.11.4/i18n/jquery-ui-i18n.js"></script>
        <script src="~/jspm_packages/npm/moment@2.10.6/moment.js"></script>
        <script asp-localized-src="~/jspm_packages/npm/moment@2.10.6/locale/*.js"></script>
        <script src="~/jspm_packages/github/twbs/bootstrap@3.3.5/js/bootstrap.js"></script>
    </environment>

    <script src="~/jspm_packages/system.js"></script>
    <script src="~/config.js"></script>
    <script>
        System.config({
            "paths": {
                "*": "spas/home/*.js"
            }
        });
        System.import('aurelia-bootstrapper');
    </script>
}